{

  "manifest_version": 2,
  "name": "Switcherhost",
  "version": "0.1",

  "description": "Redirects requests to another host, live",

  "background": {
    "scripts": ["switcherhost.js"],
  },

  "permissions": [
    "*://developer.mozilla.org/",
    "webRequest",
    "webRequestBlocking"
  ]

}
